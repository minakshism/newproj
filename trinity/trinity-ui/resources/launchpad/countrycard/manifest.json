{
	"_version": "1.15.0",
	"sap.app": {
		"id": "launchpad.countrycard",
		"type": "card",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://technical-object"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "launchpad.countrycard.View",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.38",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		}
	},
	"sap.card": {
		"type": "Component",
		"header": {
			"title": "Countries / Region / Sub-Region",
			"actions": [{
				"type": "Navigation",
				"parameters": {
					"url": "/country/index.html"
				}
			}]
		}
	}
}